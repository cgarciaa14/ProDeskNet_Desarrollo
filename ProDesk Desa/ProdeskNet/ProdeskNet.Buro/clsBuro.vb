                End If
            Next

            strProc = " sp_PDK_BURO_InsReporteBuroCuenta " & IdReporte
            For i = 0 To UBound(TL)
                If TL(i, 0) <> "" Then
                    strsql = ""
                    For j = 0 To UBound(TL, 2)
                        If j < 40 Or j > 42 Then
